# Example environment file for Statistical Tutor
# Copy this file to .env and fill in your actual values

# OpenAI API Key (required for PydanticAI agents)
OPENAI_API_KEY=your_openai_api_key_here

# Tutor Model Configuration (optional)
TUTOR_MODEL=openai:gpt-4o

# Exercise Configuration (optional)
EXERCISE_NAME=exercise-12
EXERCISES_DIR=exercises

# Tutor Mode (optional)
TUTOR_MODE=socratic

# Debug Settings (optional)
DEBUG=false
SHOW_PROMPTS=false
SHOW_REASONING=false